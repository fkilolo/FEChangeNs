{
  "name": "admin_ticket_domain",
  "author": "admin_ticket_domain",
  "description": "Frontend for ticket domain",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "start": "vite preview --mode production",
    "start:test": "vite preview --mode test",
    "preview": "vite preview",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=3000 vite build --mode production",
    "build:test": "cross-env NODE_OPTIONS=--max-old-space-size=3000 vite build --mode test",
    "buildProd": "tsc && vite build && pm2 start ecosystem.config.cjs --env production",
    "buildSTG": "tsc && vite build && pm2 start ecosystem.config.cjs --env staging",
    "stg": "serve -s dist -l 3002",
    "deploy": "serve -s dist -l 3000",
    "deploy:test": "env=test serve -s dist -l 3000"
  },
  "dependencies": {
    "@ant-design/icons": "4.8.0",
    "@ant-design/pro-components": "2.6.46",
    "@ant-design/pro-layout": "7.17.19",
    "@hookform/resolvers": "^3.10.0",
    "@reduxjs/toolkit": "1.9.3",
    "@types/recharts": "^1.8.29",
    "antd": "5.13.1",
    "async-mutex": "0.4.0",
    "axios": "1.6.5",
    "cross-env": "^7.0.3",
    "dayjs": "1.11.13",
    "dotenv": "16.0.3",
    "emoji-picker-react": "^4.11.1",
    "html-react-parser": "4.0.0",
    "insert-text-at-cursor": "^0.3.0",
    "lodash": "4.17.21",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-countup": "6.4.2",
    "react-device-detect": "2.2.3",
    "react-dom": "18.2.0",
    "react-google-charts": "^5.2.1",
    "react-hook-form": "^7.52.2",
    "react-icons": "4.8.0",
    "react-query": "^3.39.1",
    "react-quill": "2.0.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.11.2",
    "react-spinners": "0.13.8",
    "react-use": "^17.6.0",
    "recharts": "^2.12.7",
    "serve": "^14.2.3",
    "uuid": "9.0.0",
    "xlsx": "^0.18.5",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@types/insert-text-at-cursor": "^0.3.2",
    "@types/lodash": "4.14.195",
    "@types/node": "20.2.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-router-dom": "5.3.3",
    "@types/uuid": "9.0.2",
    "@vitejs/plugin-react-swc": "3.0.0",
    "rollup-plugin-visualizer": "5.9.0",
    "sass": "1.62.1",
    "typescript": "^5.6.2",
    "vite": "4.2.0"
  }
}
